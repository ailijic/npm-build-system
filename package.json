{
  "name": "npm-build-system",
  "version": "0.0.1",
  "description": "npm build system",
  "main": "index.js",
  "scripts": {
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:html",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp index.html build/",
    "clean:html": "rm -f build/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ailijic/npm-build-system.git"
  },
  "author": "ailijic",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ailijic/npm-build-system/issues"
  },
  "homepage": "https://github.com/ailijic/npm-build-system#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0"
  }
}
